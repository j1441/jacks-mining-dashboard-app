    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/j1441/jacks-mining-dashboard-app.git"
  },
  "bugs": {
    "url": "https://github.com/j1441/jacks-mining-dashboard-app/issues"
  },
